{
    "manifest_version": 3,
    "name": "Google Definition Extractor",
    "version": "1.0",
    "description": "Trích xuất định nghĩa từ Google",
    "permissions": ["activeTab", "scripting"],
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["https://www.google.com/search?q=define*"],
        "js": ["content-script.js"]
      }
    ]
  }